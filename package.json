{
  "name": "performance-board",
  "version": "0.1.0",
  "description": "Performance Board using Webpagetest.org API",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "private": true,
  "keywords": [
    "Performance",
    "Dashboard",
    "webpagetest"
  ],
  "author": "javier.rocamora@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.19.0",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-exec": "^0.4.6",
    "grunt-shell": "^1.1.2",
    "webpagetest": "^0.3.4"
  },
  "dependencies": {
    "express": "^4.13.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "rules": {
      "indent": ["warn", "tab", { "SwitchCase": 1, "VariableDeclarator": {}, "MemberExpression": 1 }],
      "quotes": ["warn", "single"],
      "max-len": ["warn", 120, 4],
      "max-params": ["warn", 4],
      "block-scoped-var": ["warn"],
      "no-shadow": ["warn"],
      "space-infix-ops": ["warn"],
      "keyword-spacing": ["warn"],
      "space-before-blocks": ["warn"],
      "spaced-comment": ["warn"],
      "space-before-function-paren": ["warn", {"anonymous": "always", "named": "never"}],
      "eqeqeq": ["warn", "smart"],
      "no-implicit-globals": ["warn"],
      "no-console": ["warn"],
      "valid-jsdoc": ["warn", {"requireReturn": false}],
      "key-spacing": ["warn", {"align": {} }]
    },
    "env": {
      "browser": true,
      "amd": true,
      "jasmine": true,
      "node": true,
      "es6": true
    }
  }
}
